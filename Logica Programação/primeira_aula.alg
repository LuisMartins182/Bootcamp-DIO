algoritmo "Primeira Aula"
// Função :
// Autor :
// Data : 10/04/2022
// Seção de Declarações 
var
nome:literal
sobrenome:literal
nomeCompleto:literal
inicio
      escreval("Qual o seu nome: ")
      leia(nome)
      escreval("Qual o seu sobrenome: ")
      leia(sobrenome)
      nomeCompleto <- nome + " " + sobrenome
      escreval(nomeCompleto)
fimalgoritmo
